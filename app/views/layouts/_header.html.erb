<div class="page-header">
  <div class="header-left">
    <h1 class="header-text">Welcome to Varmint-O-Pedia, the free critter codex that anyone can edit.</h1>
    <form class="navbar-form search" action="/versions" method="get" role="search">
      <div class="form-group">
        <input type="text" name="search_criteria" class="form-control" placeholder="Search">
      </div>
      <button type="submit" class="btn btn-default">Submit</button>
    </form>

  </div>
  <div class="header-right category-list">
    <ul>
      <% all_categories.each do |category| %>
        <li><a href="#"><%= category.name %></a></li>
      <% end %>
    </ul>
  </div>
</div>





